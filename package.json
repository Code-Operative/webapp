{
  "name": "WECOWebApp",
  "version": "1.0.0",
  "description": "The front-end AngularJS app and server for WECO.",
  "main": "server.js",
  "scripts": {
    "beanstalk:dev": "eb deploy webapp-dev --profile weco-iam",
    "beanstalk:production": "eb deploy webapp-prod --profile weco-iam",
    "build:dev": "npm-run-all -p run-gulp:dev webpack:production",
    "build:production": "npm-run-all -p run-gulp:production",
    "deploy:dev": "npm-run-all -p build:dev beanstalk:dev",
    "deploy:production": "npm-run-all -p build:production beanstalk:production",
    "lint": "eslint . .js",
    "run-gulp:dev": "gulp --env development",
    "run-gulp:local": "gulp --env development --local",
    "run-gulp:production": "gulp --env production",
    "start-server": "node server.js",
    "start": "npm-run-all -p run-gulp:local webpack:server",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run",
    "webpack:production": "webpack --config config/webpack.config.prod.js --bail",
    "webpack:server": "webpack-dev-server --config config/webpack.config.dev.js --history-api-fallback --inline --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://MSChristensen@bitbucket.org/wecollective/wecowebapp.git"
  },
  "author": "WE Collective",
  "license": "ISC",
  "homepage": "https://weco.io",
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-cache": "^4.6.0",
    "angular-chart.js": "^1.1.1",
    "angular-google-analytics": "^1.1.7",
    "angular-marked": "^1.2.2",
    "angular-mocks": "^1.6.6",
    "angular-resource": "^1.5.7",
    "angular-sanitize": "^1.5.7",
    "angular-ui-router": "^0.3.1",
    "chalk": "1.1.3",
    "chart.js": "^2.5.0",
    "dotenv": "^4.0.0",
    "es6-promise-polyfill": "^1.2.0",
    "express": "^4.13.4",
    "helmet": "^2.1.1",
    "highlightjs": "^8.7.0",
    "marked": "^0.3.6",
    "ng-file-upload": "^12.0.4",
    "npm-run-all": "^4.1.2",
    "prerender-node": "^2.4.0",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.30.1",
    "isparta-instrumenter-loader": "^1.0.1",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.6",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.6.0",
    "null-loader": "^0.1.1",
    "phantomjs": "^2.1.7",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.12.1",
    "yargs": "^7.0.2"
  }
}
